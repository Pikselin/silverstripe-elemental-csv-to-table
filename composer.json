{
    "name": "pikselin/silverstripe-elemental-csv-table",
    "type": "silverstripe-vendormodule",
    "description": "Add a table to the page from a CSV file source",
    "keywords": [
        "silverstripe", "elemental", "blocks", "table","CSV"
    ],
    "homepage" : "https://github.com/Pikselin/silverstripe-elemental-csv-to-table",
    "support" : {
      "issues" : "https://github.com/Pikselin/silverstripe-elemental-csv-to-table/issues"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pikselin",
            "email": "info@pikselin.com",
            "homepage": "https://pikselin.com"
        }
    ],
    "require": {
        "silverstripe/cms": "^4 || ^5",
        "dnadesign/silverstripe-elemental": "^4.0 || ^5"
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "installer-name": "silverstripe-elemental-csv-table"
    },
    "autoload": {
        "psr-4": {
            "Pikselin\\Elemental\\Blocks\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

